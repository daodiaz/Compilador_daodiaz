			case "boolean":
				if(!scope.getScopeManager().containsType("boolean")){
					tipe = new TypeSimple(scope, "boolean");
					tyTable.addType(tipe);
				} else {
					tipe = scopeManager.searchType("boolean");
				}
				break;
