program ::= 
  {: 
        syntaxErrorManager.syntaxInfo ("Starting parsing..."); 
   :}
  axiom:ax
  {: 
		//Aplico las instrucciones indicadas en las diapositivas para realizar la resoluci贸n de referencias simb贸licas.
		int gAddress = 10000;
		int lOffset = 0;
		List<ScopeIF> scopes = scopeManager.getAllScopes();
		for (ScopeIF scope: scopes){
			List<SymbolIF> symbols = scope.getSymbolTable().getSymbols();
			for(SymbolIF s : symbols){
				if(s instanceof SymbolVariable){
					if(scope.getLevel() == 0){
						((SymbolVariable) s).setAddress(gAddress);
						System.out.println("El tamao del tipo es: " +  s.getType().getSize());
						gAddress += s.getType().getSize();
					} else {
						((SymbolVariable) s).setAddress(lOffset);
						lOffset +=  s.getType().getSize();
						//Obtengo la funcion.
						//SymbolIF funcion = scopeManager.getSymbol(s.getScope().getName());
						//Incrementar el size de la funcion o del procedimiento en uno ya que cada campo ocupa uno sea cual sea el campo. 
					}
				}
			}
			List<TemporalIF> temporals = scope.getTemporalTable().getTemporals();
			for(TemporalIF t : temporals){
				t.setAddress(lOffset);
				lOffset++;
			}
		}
  		// No modificar esta estructura, aunque se pueden a帽adir m谩s acciones sem谩nticas
  		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilder icb = new IntermediateCodeBuilder(scope);
		
		System.out.println("La direccin global es: "+ gAddress);
		icb.addQuadruples(((Axiom_IMPL) ax).getIntermediateCode());
		icb.addQuadruple("DATA", null, gAddress - 10000);
		icb.addQuadruple("HALT");

		List<QuadrupleIF> intermediate_code = icb.create();
		
		syntaxErrorManager.syntaxInfo("An谩lisis sem谩ntico terminado ahora en copia c贸digo intermedio;");
        for (QuadrupleIF quadruple : intermediate_code) {
          syntaxErrorManager.syntaxInfo(quadruple.toString());
        }

  		finalCodeFactory.setEnvironment(CompilerContext.getExecutionEnvironment());
		finalCodeFactory.create(intermediate_code);

  		// En caso de no comentarse las dos sentencias anteriores puede generar una excepcion
  		// en las llamadas a cupTest si el compilador no est谩 completo. Esto es debido a que 
  		// a煤n no se tendr谩 implementada la generaci贸n de c贸digo intermedio ni final.
  		// Para la entrega final deber谩n descomentarse y usarse.
	
        syntaxErrorManager.syntaxInfo("Parsing process ended.");
   :};

