			semanticErrorManager.semanticFatalError("El identificador no se ha encontrado en la tabla de simbolos: " + ((Parametro)pde).getNombre());
		}

		Expresion pd = (Expresion) pde;
		
		//Instancio lo necesario parea crear el c贸digo intermedio.
		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		TemporalFactoryIF tf = new TemporalFactory(scope);
		TemporalIF pd_temp = pd.getTemporal();
		TemporalIF temp_act = tf.create();

		SentenciaAsignacion asign = null;

		if(piz instanceof SymbolBase){
			SymbolBase pi = (SymbolBase) piz;

			//Compruebo si concuerdan los tipos de la parte izquierda y derecha y qu茅 tip貌s son, seg煤n sea seteo la SentenciaAsignacion.
			if((pi.getType().getName().equals("entero")) && (pd.getType().equals("entero"))){			
				asign = new SentenciaAsignacion(pi, pd, "entero");
				System.out.println("Sentencia de asignaci贸n entera analizada");
			} else if(pi.getType().getName().equals("boolean") && pd.getType().equals("boolean")){
				asign = new SentenciaAsignacion((SymbolIF) pi,(Expresion) pd,"boolean");
				System.out.println("Sentencia de asignaci贸n booleana analizada");
			} else if(pi.getType() instanceof TypeArray){
				semanticErrorManager.semanticFatalError("No se pueden asignar directamente vectores.");
			} else
				semanticErrorManager.semanticFatalError("No se puede asignar a una variable de tipo " + ((SymbolIF) pi).getType().getName() + " una expresi贸n de tipo " + ((Expresion) pd).getType());

			//Creo la variable a la que se le va a guardar el temporal de la expresi贸n.
			Variable variable = new Variable(pi.getName(), scope);
			
			//Creo el c贸digo intermedio.
			icb.addQuadruples(pd.getIntermediateCode());
			//OJO! guardo en la direcci贸n de la variable el temporal.
			icb.addQuadruple("STA", variable, pd_temp);

			//Seteo el temporal y el codigo intermedio en asignacion.
			asign.setIntermediateCode(icb.create());
		} else {
			String pi = ((TypeArray)((Array) piz).getSymbol().getType()).getTypeSimple().getName();
			
			//Compruebo si concuerdan los tipos de la parte izquierda y derecha y qu茅 tip貌s son, seg煤n sea seteo la SentenciaAsignacion.
			if((pi.equals("entero")) && (pd.getType().equals("entero"))){			
				asign = new SentenciaAsignacion(((Array) piz).getSymbol(), pd, "entero");
				System.out.println("Sentencia de asignaci贸n entera analizada");
			} else if(pi.equals("boolean") && pd.getType().equals("boolean")){
				asign = new SentenciaAsignacion(((Array) piz).getSymbol(),(Expresion) pd,"boolean");
				System.out.println("Sentencia de asignaci贸n booleana analizada");
			} else
				//System.out.println(((SymbolIF) pi).getType().getName() + "     tiposimple" + ((TypeArray) ((SymbolIF) pi).getType()).getTypeSimple().getName);
				semanticErrorManager.semanticFatalError("No se puede asignar a una variable de tipo " + pi + " una expresi贸n de tipo " + ((Expresion) pd).getType());
			
			icb.addQuadruples(((Array) piz).getIntermediateCode());
			icb.addQuadruples(pd.getIntermediateCode());
			icb.addQuadruple("STP", ((Array) piz).getTemporal(), pd_temp);
			
			//Seteo el temporal y el codigo intermedio en asignacion.
			asign.setIntermediateCode(icb.create());
		}
		
		//Guardo true en el simbolo de la variable para saber que ya se ha inicializado.
		if(piz instanceof SymbolVariable)
			((SymbolVariable) piz).setIsInit(true);
		else if(piz instanceof Array)
			((SymbolVariable)((Array) piz).getSymbol()).setIsInit(true);

		RESULT = asign;
	:}; 
