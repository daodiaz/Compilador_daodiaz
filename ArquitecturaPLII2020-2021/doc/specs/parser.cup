 //Definici贸n de tipos
deftipos ::=
	IDEN:id ASIGNACION DECLVECTOR CIZQUIERDO valorango:rang_inf RANGO valorango:rang_sup CDERECHO ASIGNACIONVECTOR tipovector:tipo  DSENTENCIAS deftipos
	{:
		System.out.println("Empezando a analizar l铆nea de tipos");
		//Obtengo el escope y la tabla de simbolos y de tip貌s.
		ScopeIF scope = scopeManager.getCurrentScope();
		SymbolTableIF syTable = scope.getSymbolTable();
		TypeTableIF tyTable = scope.getTypeTable();
		TypeIF tipe = null;
		Integer rango_inf = ((PrimitivaAritmetica) rang_inf).getValor();
		Integer rango_sup = ((PrimitivaAritmetica) rang_sup).getValor();

		//Obtengo el nombre del identificador del tipo.
		String nombre_tipo = ((TokenIF) id).getLexema();
		//Copmpruebo que no exista ya un tipo con este nombre.
		if(scope.getScopeManager().containsType(nombre_tipo)){
			semanticErrorManager.semanticFatalError("Ya existe un tipo de usuario creado con el nombre: " + nombre_tipo);
		}
		//Obtengo tipo simple que forma parte del compuesto del usuario.
		if(!scope.getScopeManager().containsType((String) tipo)){
			if(tipo.equals("entero")){
				tipe = new TypeSimple(scope, "entero");
				tyTable.addType(tipe);
			} else if(tipo.equals("boolean")){
				tipe = new TypeSimple(scope, "boolean");
				tyTable.addType(tipe);
			} else {
				semanticErrorManager.semanticFatalError("Los tipos compuestos s贸lo pueden estar formados por tipos enteros o l贸gicos");
			}
		} else {
			tipe = tyTable.getType((String) tipo);
		}
		//Calculo el rango del tipo declarado
		int rango = (int) rango_sup - (int) rango_inf;
		//Instancio el  nuevo tipo y lo a帽ado a la tabla de tipos.
		TypeArray tipo_usuario = new TypeArray(scope, nombre_tipo);
		//Seteo el rango del tipo a帽adido y de su tipo base.
		tipo_usuario.setRango(rango + 1);
		tipo_usuario.setTypeSimple(tipe);
		tyTable.addType(tipo_usuario);
		System.out.println("Vector de tipo " + nombre_tipo + " de rango " + rango + " de tipo simple " + tipe.getName());
	:} 
	| epsilon;
tipovector ::=
	BOOLEANO
	{:
		System.out.println("Analizando declaraci贸n de vector de booleanos");
		RESULT = "boolean";
	:} 
	| ENTERO
	{:
		System.out.println("Analizando declaraci贸n de vector de enteros");
		RESULT = "entero";
	:};
valorango ::= 
	NUM:num
	{:
		//Declaro las instancias necesarias para a帽adir el c贸digo intermedio.
		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		TemporalFactoryIF tf = new TemporalFactory(scope);
		TemporalIF temp_act = tf.create();
			
		//Creo el c贸digo intermedio.
		Value valor = new Value(Integer.parseInt(((TokenIF) num).getLexema()));
		icb.addQuadruple("MV", temp_act, valor);
		
		//Creo la primitiva.	
		PrimitivaAritmetica rango = new PrimitivaAritmetica(null,null,null);
		rango.setValor(Integer.parseInt(((TokenIF) num).getLexema()));

		//Seteo el temporal y el codigo intermedio de la primitiva.
		rango.setTemporal(temp_act);
		rango.setIntermediateCode(icb.create());

		RESULT = rango;
	:} 
	| IDEN:id
	{:
		//Tengo que obtener el valor del identificador para pasarselo para calcular el rango del tipo.
		//Obtengo la scope y tabla de s铆mbolos.
		ScopeIF scope = scopeManager.getCurrentScope();
		SymbolTableIF syTable = scope.getSymbolTable();
		String nombre_rango = ((TokenIF) id).getLexema();
		//Compruebo que el identificador exista con anterioridad.
		if(!scopeManager.containsSymbol(nombre_rango)){
			semanticErrorManager.semanticFatalError("No se puede establecer el rango debido a que no ha sido declarado con anterioridad el simbolo: " + nombre_rango);
		}
		//Obtengo el s铆mbolo.
		SymbolIF rango = scopeManager.searchSymbol(nombre_rango);
		int valor = -1;
		//Compruevo que el simbolo sea de tipo aritm茅tico.
		if(!rango.getType().getName().equals("entero")){
			semanticErrorManager.semanticFatalError("S贸lo se pueden establecer rangos con simbolos enteros, actualmente es: " + rango.getType().getName());
		}
		//Compruebo que tenga valor asociado.
		if(rango instanceof SymbolVariable){
			if(((SymbolVariable) rango).getValue() == null){
				semanticErrorManager.semanticFatalError("Aunque el simbolo " + nombre_rango + " entero est谩 declarado con anterioridad, a煤n no se le ha asignado un valor, actualmente es null, no se puede crear un rango de dimensi贸n null");
			}
			//Obtengo el valor del simbolo entero obtenido.
			valor = Integer.parseInt(((SymbolVariable) rango).getValue());
		} else if(rango instanceof SymbolConstant) {
			if(((SymbolConstant) rango).getValue() == null){
				semanticErrorManager.semanticFatalError("Aunque el simbolo " + nombre_rango + " entero est谩 declarado con anterioridad, a煤n no se le ha asignado un valor, actualmente es null, no se puede crear un rango de dimensi贸n null");
			}
			valor = Integer.parseInt(((SymbolConstant) rango).getValue());
		} else {
			semanticErrorManager.semanticFatalError("En este punto s贸lo se espera un s铆mboloo variable o constante, no de otro tipo");
		}

		//Declaro las instancias necesarias para a帽adir el c贸digo intermedio.
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		TemporalFactoryIF tf = new TemporalFactory(scope);
		TemporalIF temp_act = tf.create();
			
		//Creo el c贸digo intermedio.
		Value val = new Value(valor);
		icb.addQuadruple("MV", temp_act, val);
		
		//Creo la primitiva.	
		PrimitivaAritmetica rang = new PrimitivaAritmetica(null,null,null);
		rang.setValor(valor);

		//Seteo el temporal y el codigo intermedio de la primitiva.
		rang.setTemporal(temp_act);
		rang.setIntermediateCode(icb.create());

		RESULT = rang;
	:} 
	| miembrovector:mv
	{:
		//Miembro vector devuelve una primitiva aritmetica o logica, solo si es entera obtengo su valor y lo devuelvo.
		//Declaro las instancias necesarias para a帽adir el c贸digo intermedio.
		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		TemporalFactoryIF tf = new TemporalFactory(scope);
		
		if(mv instanceof PrimitivaAritmetica) {
			TemporalIF temp_r = ((PrimitivaAritmetica) mv).getTemporal();
			TemporalIF temp_act = tf.create();
			
			//Creo el codigo intermedio.
			icb.addQuadruples(((PrimitivaAritmetica) mv).getIntermediateCode());
			icb.addQuadruple("MV", temp_act, temp_r);
			
		
			//Creo la primitiva.	
			PrimitivaAritmetica vector = new PrimitivaAritmetica(null,null,null);
		
			//Seteo el temporal y el codigo intermedio de la primitiva.
			vector.setTemporal(temp_act);
			vector.setIntermediateCode(icb.create());

			RESULT = vector;
		} else if(mv instanceof Array){
			System.out.println("Accediendo a vector mediante vector.");
			
			Array arr = (Array) mv;
			
			PrimitivaAritmetica vector = new PrimitivaAritmetica(null,null,null);
			
			TemporalIF temp_r = arr.getTemporal();
			TemporalIF temp_act = tf.create();
			
			//Creo el codigo intermedio.
			icb.addQuadruples(arr.getIntermediateCode());
			icb.addQuadruple("MV", temp_act, temp_r);
			
			//Creo la primitiva.	
			PrimitivaAritmetica vect = new PrimitivaAritmetica(null,null,null);
		
			//Seteo el temporal y el codigo intermedio de la primitiva.
			vect.setTemporal(temp_act);
			vect.setIntermediateCode(icb.create());

			System.out.println("Finalizando acceso a vector mediante vector.");
			
			RESULT = vect;	
		}
		else
			semanticErrorManager.semanticFatalError("S贸lo se pueden instanciar rangos con enteros, actualmente la expresi贸n vectorial es: " + mv.getClass().getSimpleName());
		
	:};
