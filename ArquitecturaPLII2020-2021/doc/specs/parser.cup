selectorvalororeferencia ::=  
	expresion:exp
	{:
		//PENDIENTE CAMBIAR PRIMITIVA PARA GUARDAR LEXEMA EN CASO DE SER ID, CREAR PARAMETRO A TRAVES DE ESE ID EN EL CASO DE QUE LA EXPRESION SEA UNA PRIMITIVA DE ID, SINO NULL y CRIBAR EN LLAMADA A FUNCION SI ES NULL.
		//System.out.println("Obteniedno lexema de declaraci贸n de variable: " + ((TokenIF) id).getLexema());
		Parametro parametro = null;
		//Compruebo el tipo de expresi贸n que se me devuelve.
		//Si es una expresion aritmetica estamos ante un parametro de llamada a funci贸n de tipo entero
		System.out.println("Valor por referencia de clase: " + exp.getClass().getSimpleName());
		
		if(exp instanceof ExpresionAritmetica){
			parametro = new Parametro("entero");		
		} else if(exp instanceof PrimitivaAritmetica ){
			System.out.println("Nombre valor por referencia: " + ((PrimitivaAritmetica) exp).getNombre());
			parametro = new Parametro("entero");
			if(((PrimitivaAritmetica) exp).getNombre() != null){
				parametro.setNombre(((PrimitivaAritmetica) exp).getNombre());
			}
			System.out.println("Nombre de parametro: " + parametro.getNombre());		
		} else if (exp instanceof ExpresionLogica) {
			parametro = new Parametro("boolean");
		} else if(exp instanceof PrimitivaLogica) {
			parametro = new Parametro("boolean");	
			if(((PrimitivaLogica) exp).getNombre() != null){
				parametro.setNombre(((PrimitivaLogica) exp).getNombre());
			}	
		//En caso de que estemos ante un Parametro es posible que estemos en una declaraci贸n de una l铆nea. Devuelvo el par谩metro.
		} else if(exp instanceof Parametro){
			parametro = (Parametro) exp;
		} else {
			semanticErrorManager.semanticFatalError("S贸lo se pueden utilizar como par谩metros o variables enteros o tipos l贸gicos, actualmente: " + exp.getClass().getSimpleName());	
		}
		
		//Creo el cdigo intermedio.
		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		
		icb.addQuadruples(((NonTerminal) exp).getIntermediateCode());
		icb.addQuadruple("PARAM", ((NonTerminal) exp).getTemporal());

		parametro.setIntermediateCode(icb.create());
				
		RESULT = parametro;
	:}
	|	PARAMETROVALOR IDEN:id
	{:
		System.out.println("Obteniedno lexema de declaraci贸n de variable: " + ((TokenIF) id).getLexema());
		String lexema = ((TokenIF) id).getLexema();
		//Creo el par谩metro.
		Parametro parametro = new Parametro(lexema, true);
		
		//Creo el cdigo intermedio.
		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		
		icb.addQuadruple("PARAM", new Value(lexema));

		parametro.setIntermediateCode(icb.create());
				
		RESULT = parametro;
	:};	
