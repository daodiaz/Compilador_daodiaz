// devolver
sentenciaDevolver ::= 
	FRETURN expresion:exp DSENTENCIAS
	{:
		if(exp instanceof Parametro)
			semanticErrorManager.semanticFatalError("No se ha encontrado el simbolo en la sentencia devolver: " + ((Parametro) exp).getNombre());

		ScopeIF scope = scopeManager.getCurrentScope();
		IntermediateCodeBuilderIF icb = new IntermediateCodeBuilder(scope);
		TemporalFactoryIF tf = new TemporalFactory(scope);
		TemporalIF temp_act = tf.create();
		TemporalIF exp_temp = ((Expresion) exp).getTemporal();

		//Establezco que la funcion llega a una sentencia devolver.
		String tipo = ((Expresion) exp).getType();
		System.out.println("LLegada a sentencia devolver de tipo: " + ((Expresion) exp).getType());
		SymbolFunction.addTipoDevolver(tipo);
		SymbolFunction.setDevolverDeclarado(true);
		
		//Creo el c贸digo intermedio.
		icb.addQuadruples(((Expresion) exp).getIntermediateCode());
		icb.addQuadruple("MV", temp_act, exp_temp);
		icb.addQuadruple("RET", temp_act);

		//Creo la sentencia de devolver.
		SentenciaDevolver devolver = new SentenciaDevolver();
		
		//Seteo los temporales y el codigo intermedio de la sentencia.
		devolver.setTemporal(temp_act);
		devolver.setIntermediateCode(icb.create());
		
		RESULT = devolver;
	:};



